   "source": [
    "function SQRT(x)\n",
    "      d = 0.01\n",
    "      range = ceil(x)/d\n",
    "      for i=0:range\n",
    "            xx = i*d\n",
    "            if xx*xx>x\n",
    "                  return xx-0.005\n",
    "            end\n",
    "      end\n",
    "end;"
   ]
