   "source": [
    "function SUM(value...)\n",
    "       avg = 0.0\n",
    "       count = 0\n",
    "       for v in value\n",
    "               avg = avg + v\n",
    "               count = count + 1\n",
    "       end\n",
    "      return avg/count\n",
    "end"
   ]
